{
  "name": "takehome",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext .js,.ts,.vue --ignore-path ./.gitignore .",
    "lint:js:fix": "eslint --ext .js,.ts,.vue --ignore-path ./.gitignore --fix",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint:style:fix": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore --fix",
    "lint": "npm run lint:js",
    "lint:fix": "npm run lint:js:fix && npm run lint:style:fix",
    "storybook": "npx nuxt storybook"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "core-js": "^3.23.3",
    "nuxt": "^2.15.8"
  },
  "devDependencies": {
    "@nuxt/postcss8": "^1.1.3",
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^10.0.0",
    "@nuxtjs/storybook": "^4.3.2",
    "@nuxtjs/stylelint-module": "^4.1.0",
    "@nuxtjs/svg": "^0.4.0",
    "@storybook/test-runner": "^0.5.0",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest-image-snapshot": "^5.1.0",
    "postcss": "^8.4.14",
    "postcss-html": "^1.5.0",
    "prettier": "^2.7.0",
    "prettier-plugin-tailwindcss": "^0.1.12",
    "storybook-addon-pseudo-states": "^1.15.1",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-vue": "^1.4.0",
    "tailwindcss": "^3.1.6",
    "ts-node": "^10.9.1"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "npm run lint:js:fix",
    "*.{css,vue}": "npm run lint:style:fix"
  }
}
